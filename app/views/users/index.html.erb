<%= render partial: 'layouts/nav' %>

<div class="container">

<p id="notice"><%= notice %></p>


<h1>Listing Users</h1>

<%= link_to 'New User', new_user_path, class: 'btn btn-lg' %>


<table class="table">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


</div>
